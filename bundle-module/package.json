    "rollup-plugin-terser": "^5.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/plugin-transform-typescript": "^7.6.3",
    "@babel/preset-env": "^7.6.3",
    "@types/got": "^9.6.7",
    "@types/jest": "^24.0.20",
    "concurrently": "^5.0.0",
    "lerna": "^3.18.3",
    "nodemon": "^1.19.4",
    "rollup": "^1.25.2",
    "typescript": "^3.6.4"
  }
